From e18d94622785f0703ead348df273bec6c848f04d Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Wed, 4 Dec 2013 10:43:40 -0600
Subject: [PATCH 3/3] ARM: dts: beagle-xm: make sure dvi is enabled

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 arch/arm/boot/dts/omap3-beagle-xm-ab.dts | 11 ++++++-----
 arch/arm/boot/dts/omap3-beagle-xm-c.dts  | 11 ++++++-----
 arch/arm/boot/dts/omap3-beagle-xm.dts    | 11 ++++++-----
 3 files changed, 18 insertions(+), 15 deletions(-)

diff --git a/arch/arm/boot/dts/omap3-beagle-xm-ab.dts b/arch/arm/boot/dts/omap3-beagle-xm-ab.dts
index b201d55..7b6f565 100644
--- a/arch/arm/boot/dts/omap3-beagle-xm-ab.dts
+++ b/arch/arm/boot/dts/omap3-beagle-xm-ab.dts
@@ -207,14 +207,14 @@
 
 &twl_gpio {
 	ti,use-leds;
-	/* pullups: BIT(1) */
-	ti,pullups = <0x000002>;
+	/* pullups: BIT(1), BIT(2)  */
+	ti,pullups = <0x000006>;
 	/*
 	 * pulldowns:
-	 * BIT(2), BIT(6), BIT(7), BIT(8), BIT(13)
+	 * BIT(6), BIT(7), BIT(8), BIT(13)
 	 * BIT(15), BIT(16), BIT(17)
 	 */
-	ti,pulldowns = <0x03a1c4>;
+	ti,pulldowns = <0x03a1c0>;
 };
 
 &usb_otg_hs {
@@ -276,7 +276,8 @@
 
 	tfp410: encoder@0 {
 		compatible = "ti,tfp410";
-		gpios = <&twl_gpio 2 GPIO_ACTIVE_HIGH>;
+		/* at somepoint this will work... */
+		/* gpios = <&twl_gpio 2 GPIO_ACTIVE_HIGH>; */
 
 		ports {
 			#address-cells = <1>;
