From b9943d3ad713a46cef134e95a644f64cc608dcb7 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Wed, 20 Nov 2013 11:30:56 -0600
Subject: [PATCH] omap3-beagle-xm: enable HubPower

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 arch/arm/boot/dts/omap3-beagle-xm.dts | 7 +++++++
 1 file changed, 7 insertions(+)

diff --git a/arch/arm/boot/dts/omap3-beagle-xm.dts b/arch/arm/boot/dts/omap3-beagle-xm.dts
index 4ccceb0..0549768 100644
--- a/arch/arm/boot/dts/omap3-beagle-xm.dts
+++ b/arch/arm/boot/dts/omap3-beagle-xm.dts
@@ -196,6 +196,13 @@
 	bus-width = <8>;
 };
 
+&vaux2 {
+	regulator-name = "HubPower";
+	regulator-min-microvolt = <1800000>;
+	regulator-max-microvolt = <1800000>;
+	regulator-always-on;
+};
+
 &mmc2 {
 	status = "disabled";
 };
-- 
